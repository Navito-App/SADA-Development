<div class="almond-page-content">
    <!-- Header Banner -->
    <section class="almond-block page-banner 
        {{#frontpagenavchoice2}} page-banner-padding-top {{/frontpagenavchoice2}}">
        <div class="rounded-3">
            <div class="page-banner-content">
                <div class="container">
                    <div class="banner-image-1" 
                        {{#page01img}} style="background-image:url({{page01img}});"{{/page01img}}>                   
                        <div class="row align-items-center">
                            <div class="col-lg-6">
                                <ul class="page-breadcrumb unordered-list">
                                    <li>
                                        <a href="?">{{#str}}home, moodle {{/str}}</a>
                                    </li>
                                    <li>
                                        <a href="#">{{#str}}pages, lesson {{/str}}</a>
                                    </li>
                                    <li>
                                        {{page01navbar}}
                                    </li>
                                </ul>
                                <h2 class="banner-big-title">{{page01header}}</h2>
                                <div class="banner-description"> {{page01caption}}
                                </div>
                            </div>
                        </div>
                    
                    </div>

                </div>
            </div>
        </div>
    </section>
    <!-- Header Banner End -->
    <!-- About us -->
    <section class="about-us-block mt-0">
        <div class="container">
            <div class="row align-items-center">
                <div class="col-xl-6 col-lg-6">
                    {{#page01s1img}}
                    <div class="image-widget wow fadeInLeft">
                        <img src="{{page01s1img}}" alt="about">
                    </div>
                    {{/page01s1img}}
                </div>
                <div class="col-xl-6 col-lg-6 section-heading">
                    <div class="heading-text">
                        {{page01s1header}}
                    </div>
                    <h2 class="heading-text-ww wow fadeInRight">{{page01s1title}}</h2>
                    <div class="heading-description-ww wow fadeInLeft">{{page01s1caption}}</div>
                    <div class="it-about-3-mv-box">
                        <div class="row">
                            <div class="col-xl-12 mb-5">
                                <div class="about-us-list unordered-list wow fadeInRight it-about-4-list-wrap d-flex align-items-start">
                                    <div class="about-us-item-icon">
                                        <span><i class="fa-solid fa-building-columns"></i></span>
                                    </div>
                                    <div class="about-us-item">
                                        <h5 class="about-us-item-title">{{page01s1subtitle1}}</h5>
                                        <div>{{page01s1subdefinition1}}</div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-xl-12 mb-2">
                                <div class="about-us-list unordered-list wow fadeInRight it-about-4-list-wrap d-flex align-items-start">
                                    <div class="about-us-item-icon">
                                        <span><i class="fa-solid fa-graduation-cap"></i></span>
                                    </div>
                                    <div class="about-us-item">
                                        <h5 class="about-us-item-title">{{page01s1subtitle2}}</h5>
                                        <div>{{page01s1subdefinition1}}</div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="btn-wrap d-grid gap-2 d-md-flex justify-content-md-start">
                        <a class="btn btn-primary btn-lg px-4 me-md-2" href="{{page01s1buttonlink}}" target="_blank">
                            {{#page01s1button}}
                            <span>
                                <small>{{page01s1button}}</small> 
                                <small>{{page01s1button}}</small>
                            </span>
                            {{/page01s1button}}
                        </a>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- About us End -->
    <!-- Icon Box 2 -->
    <section class="almond-block iconbox-block-2">
        <div class="container">
            <div class="text-center text-dark mb-5 wow fadeInUp" data-wow-delay=".1s">
                <div class="heading-text">
                   {{page01s2headline}}
                </div>
                <h2 class="heading-description">
                    {{page01s2title}}.
                </h2>
            </div>
            <!-- Row  -->
            <div class="row">
                {{#page01s2}}
                <div class="col-lg-4 col-xs-12 mb-4 wow fadeInUp">
                    <div class="card h-100 iconbox-card-2 rounded-3 p-3 ">
                        <div class="card-body text-dark">
                            <div class="card-content">
                                <h4 class="card-title text-dark">{{header}}</h4>
                                <div class="mt-3 text-dark card-content-desc">
                                    {{caption}}
                                </div>
                                <a class="iconbox-btn-2" href="{{link}}">
                                    <span class="btn-text-2">{{button}}</span>
                                    <span class="btn-icon">
                                        <i class="fa-solid fa-long-arrow-right"></i>
                                    </span>
                                </a>
                            </div>
                        </div>
                    </div>
                </div>
                {{/page01s2}}
            </div>
        </div>
    </section>
    <!-- Icon Box 2 End -->
    <!-- Teachers 2 -->
    <section class="almond-block teacher-block-2">
        <div class="container">
            <div class="text-center text-dark mb-5 wow fadeInUp">
                <div class="heading-text">
                    {{page01s3header}}
                </div>
                <h2 class="heading-description">
                    {{page01s3caption}}
                </h2>
            </div>
            <!-- Row  -->
            <div class="row">
                {{#page01s3}}
                <div class="col-lg-4 col-md-6 mb-4 wow fadeInUp">
                    <div class="card teacher-card">
                        <div class="teacher-image">
                            <a href="{{teacherlink}}">
                                <img src="{{userpictureURL}}" alt="user">
                            </a>
                            <ul class="social-profile unordered-list">
                                <li>
                                    {{#socialicon1}}
                                    <a href="{{sociallink1}}" target="_blank"><i class="{{socialicon1}}"></i></a>
                                    {{/socialicon1}}
                                </li>
                                <li>
                                    {{#socialicon2}}
                                    <a href="{{sociallink2}}" target="_blank"><i class="{{socialicon2}}"></i></a>
                                    {{/socialicon2}}
                                </li>
                                <li>
                                    {{#socialicon3}}
                                    <a href="{{sociallink3}}" target="_blank"><i class="{{socialicon3}}"></i></a>
                                    {{/socialicon3}}
                                </li>
                                <li>
                                    {{#socialicon4}}
                                    <a href="{{sociallink4}}" target="_blank"><i class="{{socialicon4}}"></i></a>
                                    {{/socialicon4}}
                                </li>
                            </ul>
                        </div>
                        <div class="card-content">
                            <h4 class="teacher-name">
                                <a href="{{teacherlink}}">{{teachername}}</a>
                            </h4>
                            <div class="teacher-job">{{job}}</div>
                            {{#page01s3total}}
                            <ul class="teacher-info unordered-list-center mb-0">
                                <li>
                                    <i class="fa-solid fa-people-group"></i>
                                    <span>{{studentscount}} {{#str}}student, grades{{/str}}</span>
                                </li>
                                <li>
                                    <i class="fa-solid fa-user-graduate"></i> 
                                    <span>{{coursecount}} {{#str}}courses, admin{{/str}}</span>
                                </li>
                            </ul>
                            {{/page01s3total}}
                        </div>
                    </div>
                </div>
                {{/page01s3}}
            </div>
        </div>
    </section>
    <!-- Teachers 2 End -->
    <!-- Title Box -->
    {{#block01enabled}}
    <section class="almond-block title-box-block">
        <div class="title-box"
            {{#block01color}}
            style= "background-color: {{block01color}}"
            {{/block01color}}>
            <div class="title-box-shape-1 d-none d-lg-block">
                <img src="{{block01shape1}}" alt="zuum">
            </div>
            <div class="title-box-shape-2 d-none d-lg-block">
                <img src="{{block01shape2}}" alt="zuum">
            </div>
            <div class="title-box-info">
                <div class="container">
                    <div class="row d-flex align-items-center justify-content-between">
                        <div class="col-lg-9">
                            <div class="item-content">
                                <div class="section-heading">{{{block01caption}}}</div>
                            </div>
                        </div>
                        <div class="col-lg-3 d-flex title-box-btn">
                            <a class="btn btn-primary btn-lg px-4 me-md-2" href="{{block01buttonlink}}">
                                <span>
                                    <small>{{block01button}}</small> 
                                    <small>{{block01button}}</small>
                                </span>
                            </a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    {{/block01enabled}}
    <!-- Title Box End -->
    <!-- Brands -->
    {{#block19enabled}}
    <section class="almond-block brands-block mt-0">
        <div class="container">
            <div class="text-center text-dark mb-5 wow fadeInUp" data-wow-delay=".1s">
                <div class="heading-text">
                    {{block19header}}
                </div>
                <h2 class="heading-description">
                    {{{block19caption}}}
                </h2>
            </div>
            <div class="row g-4 brands-logo unordered-list">
                {{#block19}}
                <div class="col-xl-3 col-md-4">
                    <a href="{{{link}}}">
                        {{#block19img}}
                        <img src="{{{block19img}}}" alt="brands-1">
                        {{/block19img}}  
                    </a>
                </div>
                {{/block19}} 
            </div>
        </div>
    </section>
    {{/block19enabled}}
</div>
<!-- Brands End -->